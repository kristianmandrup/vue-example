{
  "name": "vue-example",
  "version": "1.0.0",
  "description": "Vue.js example application (server-side rendering, vuex store, router, forms with contextable)",
  "main": "./src/index.js",
  "config": {
    "env": "development",
    "serverHost": "0.0.0.0",
    "serverPort": 4444,
    "publicPath": "/"
  },
  "scripts": {
    "build": "npm run build:client; npm run build:server;",
    "build:client": "node ./src/scripts/build-client.js",
    "build:server": "node ./src/scripts/build-server.js",
    "start": "node ./src/scripts/start"
  },
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "dependencies": {
    "contextable": "2.9.0",
    "express": "5.0.0-alpha.2",
    "express-vue-builder": "2.3.x",
    "isomorphic-fetch": "2.2.1",
    "vue": "2.0.x",
    "vue-builder": "1.3.x",
    "vue-contextable": "1.10.x",
    "vue-router": "2.0.x",
    "vue-server-renderer": "2.0.x",
    "vuex": "2.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.x",
    "babel-loader": "6.2.x",
    "babel-plugin-transform-object-assign": "6.8.x",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.x",
    "babel-preset-stage-3": "6.17.0",
    "css-loader": "^0.25.0",
    "express-vue-dev": "^1.2.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "inline-environment-variables-webpack-plugin": "1.1.x",
    "postcss-cssnext": "^2.8.0",
    "vue-loader": "^9.7.0",
    "vue-webpack": "^1.4.0",
    "webpack": "2.1.0-beta.25",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
